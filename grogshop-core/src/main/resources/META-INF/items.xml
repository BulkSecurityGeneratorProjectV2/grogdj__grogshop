<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.0" xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">
	
	
	<named-query name="Item.getAll">
		<query>
			SELECT
				_item
			FROM
				Item _item					
		</query>
	</named-query>
	
        
        <named-query name="Item.getAllByClub">
		<query>
			SELECT
				_item
			FROM
				Item _item
                        WHERE
                                _item.club.id  = :clubId 
		</query>
	</named-query>
        
        <named-query name="Item.getAllByUser">
		<query>
			SELECT
				_item
			FROM
				Item _item
                        WHERE
                                _item.user.id  = :userId 
		</query>
	</named-query>
		
</entity-mappings>