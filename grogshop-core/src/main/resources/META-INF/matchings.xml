<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.0" xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd">
	
	
	<named-query name="Matching.getAll">
		<query>
			SELECT
				_matching
			FROM
				Matching _matching					
		</query>
	</named-query>
        
        <named-query name="Matching.getAllByItem">
		<query>
			SELECT
				_matching
			FROM
				Matching _matching
                        WHERE
                                _matching.item.id = :itemId
		</query>
	</named-query>
		
</entity-mappings>